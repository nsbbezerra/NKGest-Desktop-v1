{
  "name": "torneadora",
  "version": "0.1.0",
  "private": false,
  "productName": "NKGEST - Sistema de Gestão Empresarial",
  "author": {
    "email": "contato.nk.info@gmail.com",
    "name": "NK Informática - Natanael Bezerra",
    "url": "https://github.com/nsbbezerra"
  },
  "license": "MIT",
  "description": "Sistema de Gestão Empresarial",
  "homepage": "./",
  "main": "public/electron.js",
  "keywords": [
    "Example",
    "React",
    "Electron"
  ],
  "dependencies": {
    "@use-it/event-listener": "^0.1.3",
    "antd": "^3.19.1",
    "axios": "^0.19.0",
    "cep-promise": "^3.0.7",
    "electron-is-dev": "^1.1.0",
    "electron-pdf-window": "^1.0.12",
    "electron-publisher-s3": "^20.17.2",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "react": "^16.8.6",
    "react-custom-scrollbars": "^4.2.1",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-highlight-words": "^0.16.0",
    "react-input-mask": "^2.0.4",
    "react-lottie": "^1.2.3",
    "react-router-dom": "4.3.1",
    "react-scripts": "3.0.1",
    "react-text-mask": "^5.4.3",
    "shortid": "^2.2.15",
    "styled-components": "^4.2.0",
    "wait-on": "^3.2.0"
  },
  "scripts": {
    "dev": "yarn react-scripts start",
    "build": "react-scripts build",
    "start": "concurrently \"cross-env BROWSER=none yarn react-scripts start\" \"wait-on http://localhost:3000 && electron .\"",
    "package-win": "electron-builder build --ia32 --x64 --win nsis -c.extraMetadata.main=build/electron.js --publish never",
    "package-linux": "electron-builder build --linux appimage --x64 -c.extraMetadata.main=build/electron.js --publish never"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "electron": "^8.0.2",
    "electron-builder": "^20.41.0",
    "electron-devtools-installer": "^2.2.4",
    "version-bump-prompt": "^5.0.1"
  },
  "build": {
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "publish": {
      "provider": "github",
      "repo": "Torneadora",
      "owner": "nkinformatica"
    },
    "win": {
      "icon": "build/sgom.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "linux": {
      "target": "AppImage",
      "icon": "build/sgom.png",
      "category": "Office"
    }
  }
}
